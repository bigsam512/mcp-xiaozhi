{
  "mcpServers": {
    "local-stdio-calculator": {
      "type": "stdio",
      "command": "python",
      "args": ["-m", "calculator"]
    },
    "playwright": {
      "command": "mcp-server-playwright",
      "args": [
        "--executable-path",
        "/usr/bin/chromium",
        "--user-data-dir",
        "/home/sam/.config/chromium"
      ]
    },
    "fetch": {
      "command": "mcp-fetch-server",
      "env": {
        "DEFAULT_LIMIT": "50000"
      }
    },
    "remote-sse-server": {
      "type": "sse",
      "url": "https://api.example.com/sse",
      "disabled": true
    },
    "remote-http-server": {
      "type": "http",
      "url": "https://api.example.com/mcp",
      "disabled": true
    }
  }
}
